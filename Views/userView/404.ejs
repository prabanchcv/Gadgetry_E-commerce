<%-include('header')%>

  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f2f2f2;
      display: block;
      justify-content: center;
      align-items: center;
      height: 100vh;
      margin: 0;
    }
    
    .error-container {
      background-color: #ffffff;
      padding: 30px;
      border-radius: 10px;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
      text-align: center;
    }
    
    h1 {
      color: #333333;
      margin: 0;
      margin-bottom: 20px;
      font-size: 36px;
    }
    
    p {
      color: #777777;
      margin: 0;
      margin-bottom: 30px;
      font-size: 18px;
    }
    
    .go-home {
      text-align: center;
    }
    
    .go-home a {
      padding: 10px 30px;
      font-size: 16px;
      background-color: #4CAF50;
      color: white;
      text-decoration: none;
      border-radius: 5px;
      transition: background-color 0.3s ease-in-out;
    }
    
    .go-home a:hover {
      background-color: #45A049;
    }
  </style>
</head>
<body>
  <div class="error-container">
    <h1>Oops! Page Not Found</h1>
    <p>We're sorry, but the page you are looking for cannot be found.</p>
    <div class="go-home">
      <a href="/">Go to Homepage</a>
    </div>
  </div>


<%-include('footer')%>