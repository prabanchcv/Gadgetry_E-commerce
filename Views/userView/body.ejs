     <!-- ekka Cart Start -->
     <div class="ec-side-cart-overlay"></div>
     <div id="ec-side-cart" class="ec-side-cart">
         <div class="ec-cart-inner">
             <div class="ec-cart-top">
                 <div class="ec-cart-title">
                     <span class="cart_title">My Cart</span>
                     <button class="ec-close">×</button>
                 </div>
                 <ul class="eccart-pro-items">
                    <% if (Array.isArray(cart) && cart.length > 0) { %>
                    <% cart.forEach(function(cart){ %>
                     <li>
                         <a href="/productView?id=<%= cart.product._id %>" class="sidekka_pro_img"><img
                                 src="<%= cart.product.imageUrl[0].url%>" alt="product"></a>
                         <div class="ec-pro-content">
                             <a href="/productView?id=<%= cart.product._id %>" class="cart_pro_title"><%= cart.product.name%></a>
                             <span class="cart-price"><span>₹<%= cart.product.price%></span> x  <%= cart.quantity %></span>
                           
                             <a href="javascript:void(0)" class="remove">×</a>
                         </div>
                     </li>
                     <% }) %>
                     <% } else { %>
                        <p>Your cart is empty.</p>
                      <% } %>
             
                 </ul>
             </div>
             <div class="ec-cart-bottom">
                 <div class="cart-sub-total">
                     <table class="table cart-table">
                         <tbody>
                             <tr>
                                 <td class="text-left">Sub-Total :</td>
                                 <td class="text-right">₹<%= subTotal %></td>
                             </tr>
                             <tr>
                                 <td class="text-left">Delivery Charge :</td>
                                 <td class="text-right"> ₹0.00</td>
                             </tr>
                             <tr>
                                 <td class="text-left">Total :</td>
                                 <td class="text-right primary-color"><%= subTotal %></td>
                             </tr>
                         </tbody>
                     </table>
                 </div>
                 <div class="cart_btn">
                     <a href="/cart" class="btn btn-primary">View Cart</a>
                  
                     <a href="/checkout" class="btn btn-secondary">Checkout</a>
                 </div>
             </div>
         </div>
     </div>
     <!-- ekka Cart End -->
    